{
  "executable": "Arcemi.Pathfinder.SaveGameEditor",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "ArcemiPathfinderSaveGameEditor",
  "author": "",
  "singleInstance": false,
  "environment": "Production",
  "build": {
    "appId": "com.Arcemi.Pathfinder.SaveGameEditor.app",
    "productName": "Arcemi Pathfinder Save Game Editor",
    "copyright": "Copyright Â© 2021",
    "buildVersion": "1.3.1",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}